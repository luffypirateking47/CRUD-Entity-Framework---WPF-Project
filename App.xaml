<Application
    x:Class="CSharp.WPF.ADO.ConnectionModels.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:CSharp.WPF.ADO.ConnectionModels"
    StartupUri="/Views/MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--  Order of is important - Lowest level first  -->
                <ResourceDictionary Source="/Styles/Colors.xaml" />
                <ResourceDictionary Source="/Styles/Fonts.xaml" />
                <ResourceDictionary Source="/Styles/Texts.xaml" />
                <ResourceDictionary Source="/Styles/Buttons.xaml" />
                <ResourceDictionary Source="/Styles/ListViews.xaml" />
                <ResourceDictionary Source="/Styles/UserControls.xaml" />
                <ResourceDictionary Source="/Styles/Pages.xaml" />
                <ResourceDictionary Source="/Styles/ContactCard.xaml" />
                <ResourceDictionary Source="/Styles/Windows.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
